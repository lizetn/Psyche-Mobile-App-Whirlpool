<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="asu.whirlpool.psychewhirlpool">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".timeline.TimelineActivity"
            android:label="@string/title_activity_timeline"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="asu.whirlpool.psychewhirlpool.MainActivity" />
        </activity>
        <activity
            android:name=".FactsActivity"
            android:label="@string/title_activity_facts"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="asu.whirlpool.psychewhirlpool.MainActivity" />
        </activity>
        <activity
            android:name=".SocialMediaActivity"
            android:label="SocialMediaActivity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="asu.whirlpool.psychewhirlpool.MainActivity" />
        </activity>
        <activity
            android:name=".GalleryActivity"
            android:label="@string/title_activity_gallery"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="asu.whirlpool.psychewhirlpool.MainActivity" />
        </activity>
        <activity
            android:name=".CountdownActivity"
            android:label="@string/title_activity_countdown" />
        <activity
            android:name=".GameActivity"
            android:label="@string/title_activity_game"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="asu.whirlpool.psychewhirlpool.MainActivity" />
        </activity>
      
        <activity
            android:name=".SocialMediaTabs"
            android:label="@string/title_activity_social_media_tabs"
            android:theme="@style/AppTheme" />
        </activity>
      
        <activity android:name=".Facebook"></activity>

        <activity
            android:name=".timeline.TimePhaseActivity"
            android:label="@string/title_activity_time_phase"
            android:parentActivityName=".timeline.TimelineActivity"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="asu.whirlpool.psychewhirlpool.timeline.TimelineActivity" />
        </activity>
  
    </application>

</manifest>